{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@auth/*": [
                "src/packages/auth/*"
            ],
            "@shared/*": [
                "src/packages/shared/*"
            ],
            "@flight/*": [
                "src/packages/flight/*"
            ],
            "@modules/*": [
                "src/modules/*"
            ],
            "@user/*": [
                "src/packages/user/*"
            ],
            "@invitation/*": [
                "src/packages/invitation/*"
            ],
            "@payment/*": [
                "src/packages/payment/*"
            ],
            "@test/*": [
                "test/*"
            ],
            "@/*": [
                "src/*"
            ]
        }
    }
}